title: $:/core/images/secrets-button
tags: $:/tags/Image

\parameters (size:"22pt")
<svg width=<<size>> height=<<size>> class="tc-image-secrets-button tc-image-button" viewBox="0 0 128 128">
  <g fill-rule="evenodd">
    <!-- Vault/Safe body -->
    <rect x="24" y="32" width="80" height="80" rx="8" ry="8" stroke="currentColor" stroke-width="4" fill="none"/>
    
    <!-- Inner vault door -->
    <rect x="32" y="40" width="64" height="64" rx="4" ry="4" stroke="currentColor" stroke-width="2" fill="none"/>
    
    <!-- Lock mechanism circle -->
    <circle cx="64" cy="72" r="20" stroke="currentColor" stroke-width="3" fill="none"/>
    
    <!-- Keyhole -->
    <path d="M64 64 A 6 6 0 0 1 64 76 A 6 6 0 0 1 64 64 Z M60 70 L60 82 L68 82 L68 70 Z" fill="currentColor"/>
    
    <!-- Secret dots (representing secured content) -->
    <circle cx="44" cy="52" r="2" fill="currentColor"/>
    <circle cx="64" cy="52" r="2" fill="currentColor"/>
    <circle cx="84" cy="52" r="2" fill="currentColor"/>
  </g>
</svg>