{
	"description": "Documentation from https://tiddlywiki.com",
	"plugins": [
		"tiddlywiki/browser-sniff",
		"tiddlywiki/confetti",
		"tiddlywiki/dynannotate",
		"tiddlywiki/internals",
		"tiddlywiki/menubar",
		"tiddlywiki/railroad",
		"tiddlywiki/tour",
		"tiddlywiki/plugin-combiner",
		"codemirror-6/editor",
		"codemirror-6/edit-text",
		"codemirror-6/auto-close-tags",
		"codemirror-6/click-navigate",
		"codemirror-6/color-picker",
		"codemirror-6/emoji-picker",
		"codemirror-6/fold",
		"codemirror-6/image-preview",
		"codemirror-6/keymap-emacs",
		"codemirror-6/keymap-vim",
		"codemirror-6/lang-cpp",
		"codemirror-6/lang-css",
		"codemirror-6/lang-csv",
		"codemirror-6/lang-go",
		"codemirror-6/lang-html",
		"codemirror-6/lang-java",
		"codemirror-6/lang-javascript",
		"codemirror-6/lang-json",
		"codemirror-6/lang-latex",
		"codemirror-6/lang-lezer",
		"codemirror-6/lang-markdown",
		"codemirror-6/lang-php",
		"codemirror-6/lang-python",
		"codemirror-6/lang-rust",
		"codemirror-6/lang-sass",
		"codemirror-6/lang-sql",
		"codemirror-6/lang-tiddlywiki",
		"codemirror-6/lang-wast",
		"codemirror-6/lang-xml",
		"codemirror-6/lang-yaml",
		"codemirror-6/line-numbers",
		"codemirror-6/link-preview",
		"codemirror-6/lint",
		"codemirror-6/snippets",
		"codemirror-6/theme-ayu",
		"codemirror-6/theme-catppuccin",
		"codemirror-6/theme-dracula",
		"codemirror-6/theme-everforest",
		"codemirror-6/theme-flexoki",
		"codemirror-6/theme-github",
		"codemirror-6/theme-greyscale",
		"codemirror-6/theme-gruvbox",
		"codemirror-6/theme-kanagawa",
		"codemirror-6/theme-material",
		"codemirror-6/theme-monokai",
		"codemirror-6/theme-nord",
		"codemirror-6/theme-one",
		"codemirror-6/theme-palenight",
		"codemirror-6/theme-rose-pine",
		"codemirror-6/theme-solarized",
		"codemirror-6/theme-tokyo-night",
		"codemirror-6/theme-zenburn",
		"codemirror-6/toolbars",
		"codemirror-6/word-count",
		"codemirror-6/zen-mode"
	],
	"themes": [
		"tiddlywiki/vanilla",
		"tiddlywiki/snowwhite",
		"tiddlywiki/starlight",
		"tiddlywiki/seamless",
		"tiddlywiki/centralised",
		"tiddlywiki/tight",
		"tiddlywiki/heavier",
		"tiddlywiki/tight-heavier",
		"tiddlywiki/readonly"
	],
	"languages": [
	],
	"build": {
		"index": [
			"--savetiddlers","[tag[external-image]]","images",
			"--render","[tag[external-text]]","[encodeuricomponent[]addprefix[text/]addsuffix[.tid]]","text/plain","$:/core/templates/tid-tiddler",
			"--setfield","[tag[external-image]]","_canonical_uri","$:/core/templates/canonical-uri-external-image","text/plain",
			"--setfield","[tag[external-text]]","_canonical_uri","$:/core/templates/canonical-uri-external-text","text/plain",
			"--setfield","[tag[external-image]] [tag[external-text]]","text","","text/plain",
			"--render","$:/core/save/all","index.html","text/plain"],
		"empty": [
			"--render","$:/editions/tw5.com/download-empty","empty.html","text/plain",
			"--render","$:/editions/tw5.com/download-empty","empty.hta","text/plain"],
		"encrypted": [
			"--password", "password",
			"--render", "$:/core/save/all", "encrypted.html", "text/plain",
			"--clearpassword"],
		"favicon": [
			"--savetiddler","$:/favicon.ico","favicon.ico",
			"--savetiddler","$:/green_favicon.ico","static/favicon.ico"],
		"readmes": [
			"--render","ReadMe","readme.md","text/html",
			"--render","ReadMeBinFolder","bin/readme.md","text/html",
			"--render","ContributingTemplate","contributing.md","text/html",
			"--render","$:/core/copyright.txt","license","text/plain"],
		"tw2": [
			"--render","TiddlyWiki2ReadMe","tw2/readme.md","text/html"],
		"static": [
			"--render","$:/core/templates/static.template.html","static.html","text/plain",
			"--render","$:/core/templates/alltiddlers.template.html","alltiddlers.html","text/plain",
			"--render","[!is[system]]","[encodeuricomponent[]addprefix[static/]addsuffix[.html]]","text/plain","$:/core/templates/static.tiddler.html",
			"--render","$:/core/templates/static.template.css","static/static.css","text/plain"],
		"external-js": [
			"--render","$:/core/save/offline-external-js","[[external-]addsuffix<version>addsuffix[.html]]","text/plain",
			"--render","$:/core/templates/tiddlywiki5.js","[[tiddlywikicore-]addsuffix<version>addsuffix[.js]]","text/plain"],
		"archive":[
			"--render","$:/core/save/all","[[archive/full/TiddlyWiki-]addsuffix<version>addsuffix[.html]]","text/plain",
			"--render","$:/editions/tw5.com/download-empty","[[archive/empty/Empty-TiddlyWiki-]addsuffix<version>addsuffix[.html]]","text/plain",
			"--render","[[TiddlyWiki Archive]]","archive/index.html","text/plain","$:/core/templates/static.tiddler.html",
			"--render","$:/core/templates/static.template.css","archive/static.css","text/plain"]
	},
	"config": {
		"retain-original-tiddler-path": true
	}
}
